alias ga='git add'
alias gb='git checkout'
alias gc='git commit'
alias gca='git add . && git commit --amend --no-edit'
alias gd='git remote prune origin && git branch --merged | xargs git branch -d'
alias gf='git fetch'
alias gm='git merge'
alias gp='git push'
alias gpo='git push -u origin'
alias gr='git rebase'
alias gs='git status'
alias gu='git branch -u origin/$(git symbolic-ref --short HEAD)'
